{
  "name": "deck-solved",
  "version": "1.0.0",
  "description": "A simple, easy-to-use and user-friendly web-application which can be used as an MTG card reference and as a deck-building tool.",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "client-test": "npm test --prefix client",
    "client-install": "npm install --prefix client"
  },
  "author": "Sergio Guidi",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^5.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.8.11",
    "qs": "^6.9.1"
  }
}
